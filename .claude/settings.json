{
  "includeCoAuthoredBy": false,
  "preferredNotifChannel": "terminal_bell",
  "permissions": {
    "allow": [
      "WebFetch",
      "Bash(ag:*)",
      "Bash(bun:*)",
      "Bash(git checkout:*)",
      "Bash(git switch:*)",
      "Bash(git add:*)",
      "Bash(git push:*)",
      "Bash(git commit:*)",
      "Bash(gh pr:*)",
      "Bash(ls:*)",
      "Bash(diff:*)",
      "Bash(find:*)",
      "Bash(osascript:*)"
    ],
    "deny": [
      "Read(.env*)",
      "Write(.env*)",
      "Bash(git reset:*)",
      "Bash(git rebase:*)",
      "Bash(sudo:*)",
      "Bash(rm:*)",
      "Bash(curl:*)"
    ]
  }
}
